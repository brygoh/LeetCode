class Solution:
    def plusOne(self, digits: List[int]) -> List[int]:
        carry = 0
        for i in range(len(digits)-1, -1, -1):
            if digits[i] == 9:
                carry = 1
                digits[i] = 0
            else:
                if i == len(digits)-1:
                    digits[i] += 1
                digits[i] += carry
                carry = 0
                break
        if carry == 1:
            return [1] + digits
        return digits